{
  "name": "lavdiu/laf",
  "description": "LAF Framework",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Lulzim",
      "email": "l@orav.net"
    }
  ],
  "config": {
    "platform": {
      "php": "8"
    },
    "vendor-dir": "vendor",
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "autoload": {
        "psr-4": {"Laf\\": "src/Laf"}
    },
  "require": {
    "php": ">=8.0",
    "ext-gd": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-pdo": "*",
    "box/spout": "^3.0",
    "claviska/simpleimage": "^4.2.0",
    "monolog/monolog": "^1.24",
    "phpoffice/phpspreadsheet": "^1.10"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^9.5",
    "rector/rector": "^2.1"
  }
}
